{
  "name": "piloteers-dashboard-pi-gateway",
  "version": "0.0.51",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "public-files": "rm -rf ./dist/public/ && mkdir ./dist/public && cp -r ./src/public/ ./dist/public/",
    "views": "cp -r ./src/views/ ./dist/views/",
    "build": "npm run public-files && npm run views && tsc && git add . && git commit -a -m \"build\" && git push && npm version patch",
    "clear": "sudo rm -rf node_modules && sudo rm -rf package-lock.json",
    "git": " git reset --hard HEAD && git pull",
    "pre-prod": "sudo npm i -g pm2 && sudo npm run git && sudo npm run clear && sudo npm i",
    "prod": "sudo pm2 start pm2.config.js --env production --name dashboard-gateway",
    "update-models": "rm -rf ./node_modules/@piloteers/dashboard-model && npm i @piloteers/dashboard-model",
    "update-device": "sudo npm run git && sudo pm2 restart all"
  },
  "dependencies": {
    "@piloteers/dashboard-model": "^1.0.137",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "ejs": "~2.6.1",
    "express": "~4.16.0",
    "express-ws": "^4.0.0",
    "http-errors": "~1.7.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "morgan": "~1.9.0",
    "query-string": "^6.4.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "socketio-wildcard": "^2.0.0",
    "systemjs": "^3.1.6",
    "uuid": "^3.3.2",
    "webpack": "^4.30.0",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^11.13.8",
    "ts-loader": "^6.0.0",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.3.2"
  }
}
